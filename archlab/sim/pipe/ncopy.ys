	jle notInc7
	iaddq $1,%rax

notInc7:
    andq %r14,%r14 # val <= 0?
	jle notInc8
	iaddq $1,%rax

notInc8:
    andq %rbx,%rbx # val <= 0?
	jle notInc9
	iaddq $1,%rax

notInc9:
	iaddq $72, %rdi		# src+=6
	iaddq $72, %rsi		# dst+=6
	iaddq $-9, %rdx		# len-=6 # len >= 0?
	jge Loop			# if so, goto Loop:

end:
    iaddq $9,%rdx
    je Done

#1
    mrmovq (%rdi), %rcx
    rmmovq %rcx, (%rsi)
    andq %rcx,%rcx # val <= 0?
	jle notIncx9
	iaddq $1,%rax
notIncx9:
    iaddq $-1,%rdx
