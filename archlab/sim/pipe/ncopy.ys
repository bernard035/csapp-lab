    mrmovq 40(%rdi), %rcx	
    rmmovq %rcx, 40(%rsi)
    andq %rcx,%rcx
    jle notIncx14
    iaddq $1,%rax
notIncx14:
#7
    iaddq $-1,%rdx
    je Done
    mrmovq 48(%rdi), %rcx	
    rmmovq %rcx, 48(%rsi)
    andq %rcx,%rcx
    jle notIncx15
    iaddq $1,%rax
notIncx15:
#8
    iaddq $-1,%rdx
    je Done
    mrmovq 56(%rdi), %rcx	
    rmmovq %rcx, 56(%rsi)
    andq %rcx,%rcx
