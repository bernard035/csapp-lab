#2
    mrmovq 8(%rdi), %r8
    rmmovq %r8, 8(%rsi)
    andq %r8,%r8
	jle notIncx10
	iaddq $1,%rax
notIncx10:

# ...and store it to dst
    iaddq $-1,%rdx
    je Done

#3
    mrmovq 16(%rdi), %r9
    rmmovq %r9, 16(%rsi)
    andq %r9,%r9
	jle notIncx11
	iaddq $1,%rax
notIncx11:
    iaddq $-1,%rdx
